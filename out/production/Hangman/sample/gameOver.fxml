<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GameOver">
   <children>
      <ImageView fitHeight="200.0" fitWidth="200.0" layoutX="150.0" layoutY="64.0" opacity="0.3" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/6.png" />
         </image>
      </ImageView>
      <Label layoutX="145.0" layoutY="34.0" prefHeight="59.0" prefWidth="210.0" text="Game Over">
         <font>
            <Font name="System Italic" size="40.0" />
         </font>
      </Label>
      <Label layoutX="178.0" layoutY="264.0" text="Final Score:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="finalScore" layoutX="292.0" layoutY="265.0" text="0">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button layoutX="178.0" layoutY="310.0" mnemonicParsing="false" onAction="#playAgain" text="Play Again">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
